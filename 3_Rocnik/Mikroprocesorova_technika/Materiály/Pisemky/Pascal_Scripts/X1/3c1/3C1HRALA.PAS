program soucetPosloupnosti;
uses crt;
var cislo1, cislo2, i, rozdil: integer;
label start;


        begin
clrscr;
start:  writeln('Zadej prvni cislo:  ');
        readln(cislo1);
        writeln('Zadej druhe cislo:  ');
        readln(cislo2);
        rozdil := cislo2 - cislo1;
        writeln('Rozdil cisel je: ',rozdil);
        i := cislo1 + 1;






readln;
        end.