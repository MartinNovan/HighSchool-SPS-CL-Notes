.include "M32def.inc"
.cseg
.org 0
reset:	jmp start		;reset vektor

;-------------------------------------------------
.org 0x100
start: 	ldi r16,0x5F	;nastaveni ukazatele
		out SPL,r16		;zasobniku na 0x085F
		ldi r16,0x08	;konec SRAM
		out SPH,r16	



konec:	rjmp konec
;-------------------------------------------------